{
  "manifest_version": 2,
  "name": "Bilibili",
  "description": "Search Engine for Bilibili",
  "version": "1.0",
  "icons": {
      "32": "icons/32.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "dc198424601@outlook.com"
    }
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Bilibili",
      "search_url": "https://search.bilibili.com/all?keyword={searchTerms}",
      "keyword": "bili",
      "favicon_url": "https://static.hdslb.com/images/favicon.ico",
      "is_default": false,
      "encoding": "UTF-8"
    }
  }
}
